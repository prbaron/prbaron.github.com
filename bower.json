{
    "name"            : "prbaron.github.com",
    "version"         : "1.0.0",
    "private"         : true,
    "ignore"          : [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies"    : {
        "bootstrap"   : "*",
        "font-awesome" : "*"
    },
    "exportsOverride" : {
        "jquery"      : {
            "_vendor" : "dist/*.js"
        },
        "bootstrap"   : {
            "_vendor" : "dist/js/*.js",
            "_less"   : "less/*.less",
            "fonts"   : "dist/fonts/*.*"
        },
        "font-awesome" : {
            "fonts" : "fonts/*",
            "_less" : "less/*"
        }
    }
}